{
    "name": "vi-lib",
    "version": "1.0.6",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/ManhattanDoctor/vi-lib.git"
    },
    "author": "Manhattan Doctor",
    "license": "MIT",
    "paths": {
        "@vichatter-sdk/*": [
            "src/*"
        ]
    },
    "scripts": {
        "build": "rm -rf dist && tsc",
        "lint": "npm run format && tslint -p tsconfig.root.json -c tslint.json && tsc --project src",
        "lint:fix": "npm run format:fix && tslint -p tsconfig.root.json -c tslint.json --fix",
        "format": "prettier --list-different \"src/**/*.{ts,js,json}\"",
        "format:fix": "prettier --write \"src/**/*.{ts,js,json}\""
    },
    "dependencies": {
        "@angular/animations": "7.1.1",
        "@angular/cdk": "7.1.0",
        "@angular/common": "7.1.1",
        "@angular/core": "7.1.1",
        "@angular/forms": "7.1.1",
        "@angular/material": "7.1.0",
        "@angular/material-moment-adapter": "7.1.0",
        "@angular/platform-browser": "7.1.1",
        "@angular/router": "7.1.1",
        "@nguniversal/express-engine": "7.0.2",
        "@ngx-translate/core": "11.0.1",
        "interactjs": "1.3.4",
        "moment": "2.22.2",
        "ngx-cookie": "4.1.2",
        "prettier": "1.15.3",
        "rxjs": "6.3.3",
        "tsconfig-paths": "3.7.0",
        "tslint": "5.11.0",
        "typescript": "3.2.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {}
}
